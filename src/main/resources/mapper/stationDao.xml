<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.trainManageSystem.dao.StationDao">


    <select id="selectStationByName" resultType="Station" parameterType="String">
        select * from station where station_name like #{name}
    </select>

</mapper>